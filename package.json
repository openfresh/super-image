{
  "name": "super-image",
  "version": "0.0.0",
  "description": "React image component with LazyLoad and object-fit",
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib dist && npm run js && npm run css",
    "js": "NODE_ENV=development babel src --out-dir lib --ignore *.test.js",
    "css": "postcss src/index.css --use postcss-cssnext --output dist/super-image.css",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:js": "eslint src",
    "lint:css": "stylelint src/*.css",
    "test": "npm run lint && karma start karma.conf.js --single-run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openfresh/super-image.git"
  },
  "keywords": [
    "react",
    "viewport",
    "intersection",
    "object-fit",
    "lazyload",
    "image"
  ],
  "author": "openfresh",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/openfresh/super-image/issues"
  },
  "homepage": "https://github.com/openfresh/super-image#readme",
  "dependencies": {
    "bem-classnames": "^1.0.7",
    "object.assign": "^4.0.4",
    "viewport-observer": "^0.7.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-power-assert": "^1.0.0",
    "babel-preset-react": "^6.24.1",
    "babelify": "^7.3.0",
    "browserify": "^14.4.0",
    "eslint": "^4.3.0",
    "eslint-config-fresh": "^2.1.0",
    "eslint-config-fresh-react": "^2.1.0",
    "eslint-config-xo-react": "^0.13.0",
    "eslint-config-xo-space": "^0.16.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.1.0",
    "karma": "^1.7.0",
    "karma-browserify": "^5.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "mocha": "^3.5.0",
    "postcss-cli": "^4.1.0",
    "postcss-cssnext": "^3.0.2",
    "power-assert": "^1.4.4",
    "stylelint": "^8.0.0",
    "stylelint-config-fresh": "^2.0.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-order": "^0.6.0"
  },
  "peerDependencies": {
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1"
  }
}
